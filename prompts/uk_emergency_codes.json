	"ambulance_codes": {
		"response_categories": {
			"description": "NHS ambulance response categories implemented across UK since 2017",
			"codes": {
				"Category 1": {
					"description": "Immediate life-threatening conditions (cardiac arrest, choking, unconscious, continuous seizure, major trauma, severe allergic reactions)",
					"response_time_target": "7 minutes (average)",
					"equipment": "Always uses lights and sirens"
				},
				"Category 2": {
					"description": "Emergency calls (stroke, chest pain, major burns, sepsis, road traffic collisions)",
					"response_time_target": "18 minutes (average), 40 minutes (90th percentile)",
					"equipment": "Always uses lights and sirens"
				},
				"Category 3": {
					"description": "Urgent calls (falls, diabetic problems, isolated limb fractures, abdominal pain)",
					"response_time_target": "120 minutes (90th percentile)",
					"equipment": "Usually uses lights and sirens (service policy dependent)"
				},
				"Category 4": {
					"description": "Less urgent calls (diarrhoea, vomiting, non-traumatic back pain)",
					"response_time_target": "180 minutes (90th percentile)",
					"equipment": "Not usually lights and sirens"
				},
				"Category 5": {
					"description": "Calls managed through hear and treat / telephone advice",
					"response_time_target": "Telephone assessment only",
					"equipment": "No physical response"
				},
				"GP Urgent": {
					"description": "GP urgent admissions, urgent inter-hospital transfers",
					"response_time_target": "1-4 hours or scheduled timeframe",
					"equipment": "Not usually lights and sirens"
				}
			}
		},
